{
  "manifest_version": 2,

  "name": "sleeperTab",
  "description": "Extension to close out a tab after a given time",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "bell.png",
    "default_popup": "menu.html"
  },
  "permissions": ["tabs", "activeTab", "alarms"]
}
